{
    "manifest_version": 2,
    "name": "Watcard Balance Viewer",
    "version": "1.0",
    "description": "A Chrome extension to view Watcard balances.",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "https://account.watcard.uwaterloo.ca/*"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://watcard.uwaterloo.ca/OneWeb/Account/LogOn"],
        "js": ["contentScript.js"]
      }
    ]
  }
  